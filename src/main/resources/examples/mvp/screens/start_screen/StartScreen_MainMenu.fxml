<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="300.0" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="examples.mvp.controllers.start_screen.MainMenuController">
   <children>
      <Label fx:id="MM_Label" alignment="TOP_CENTER" text="Main Menu" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="44.0" />
         </font>
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
      <Button fx:id="newFile" mnemonicParsing="false" onAction="#onAction_NewFile" prefWidth="150.0" text="New" textAlignment="CENTER">
         <font>
            <Font size="38.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Button>
      <Button fx:id="openFile" mnemonicParsing="false" onAction="#onAction_OpenFile" prefWidth="150.0" text="Open" textAlignment="CENTER">
         <font>
            <Font size="38.0" />
         </font>
      </Button>
   </children>
</VBox>
